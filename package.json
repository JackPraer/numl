{
  "name": "nude",
  "version": "0.1.0",
  "module": "dist/nude.js",
  "dependencies": {
    "ms": "^2.0.0"
  },
  "devDependencies": {
    "cssnano": "^4.1.10",
    "inline-assets": "^1.4.3",
    "rollup": "^1.0.0",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "size-limit": "^1.3.7",
    "@babel/core": "7.2.0",
    "parcel-bundler": "^1.6.1"
  },
  "scripts": {
    "build": "rollup -c && size-limit",
    "dev": "rollup -c -w",
    "test": "parcel --out-dir test test.html --open",
    "size": "size-limit"
  },
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "name": "nude",
      "webpack": false,
      "path": [
        "dist/nude.js"
      ]
    },
    {
      "name": "nude global css",
      "webpack": false,
      "path": [
        "dist/nude.global.css"
      ]
    },
    {
      "name": "nude modifiers css",
      "webpack": false,
      "path": [
        "dist/nude.modifiers.css"
      ]
    },
    {
      "name": "nude pack",
      "webpack": false,
      "path": [
        "dist/nude.pack.js",
        "dist/nude.pack.css"
      ]
    }
  ]
}
