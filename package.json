{
  "name": "numl",
  "version": "0.7.0",
  "module": "dist/numl.js",
  "homepage": "https://github.com/tenphi/nude#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tenphi/nude.git"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "css-color-function": "^1.3.3",
    "cssnano": "^4.1.10",
    "inline-assets": "^1.4.3",
    "install": "^0.13.0",
    "npm": "^6.12.0",
    "parcel-bundler": "^1.12.4",
    "rollup": "^1.25.1",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "size-limit": "^1.3.8"
  },
  "scripts": {
    "build": "mkdir -p dist && rm -vrf dist/* && rollup -c && size-limit",
    "build-only": "mkdir -p dist && rm -vrf dist/* && rollup -c",
    "dev": "rollup -c -w",
    "play": "mkdir -p test && rm -vrf test/* && parcel --out-dir test index.html --open",
    "build-play": "mkdir -p playground && rm -vrf playground/* && parcel build --out-dir playground index.html",
    "now": "npm run build-play && now --prod playground --name nude-playground",
    "size": "size-limit",
    "start": "npm run play",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "size-limit": [
    {
      "name": "numl",
      "webpack": false,
      "path": [
        "dist/numl.js"
      ]
    },
    {
      "name": "numl module",
      "webpack": false,
      "path": [
        "dist/numl.module.js"
      ]
    },
    {
      "name": "colors module",
      "webpack": false,
      "path": [
        "dist/numl.colors.js"
      ]
    }
  ]
}
